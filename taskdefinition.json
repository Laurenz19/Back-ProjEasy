{
    "containerDefinitions": [
        {
            "name": "waiz-in-back-app-first-task",
            "image": "<IMAGE_NAME>",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "cicd-backend-logs",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs-backend"
                }
            }
        }
    ],
    "family": "waiz-in-back-app-first-task",
    "taskRoleArn": "arn:aws:iam::649952013762:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::649952013762:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "status": "ACTIVE",
    "compatibilities": [
        "EC2",
        "FARGATE"
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048",
    "tags": [
        {
            "key": "Name",
            "value": "BlueTaskDefinition"
        }
    ]
}